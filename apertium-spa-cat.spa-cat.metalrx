      <match lemma="asesorar" tags="vblex.pp.m.sg"/>
      <match lemma="autorizar" tags="vblex.pp.m.sg"/>
      <match lemma="cansar" tags="vblex.pp.m.sg"/>
      <match lemma="concernir" tags="vblex.pp.m.sg"/>
      <match lemma="contentar" tags="vblex.pp.m.sg"/>
      <match lemma="desautorizar" tags="vblex.pp.m.sg"/>
      <match lemma="entrevistar" tags="vblex.*"/> 
      <match lemma="felicitar" tags="vblex.pp.m.sg"/>   
      <match lemma="fichar" tags="vblex.pp.m.sg"/>
      <match lemma="homenajear" tags="vblex.pp.m.sg"/>
      <match lemma="informar" tags="vblex.pp.m.sg"/>
      <match lemma="perjudicar" tags="vblex.pp.m.sg"/>
      <match lemma="preocupar" tags="vblex.pp.m.sg"/>
      <match lemma="promover" tags="vblex.pp.m.sg"/>
      <match lemma="reñir" tags="vblex.pp.m.sg"/>
      <match lemma="reunir" tags="vblex.pp.m.sg"/>
      <match lemma="secuestrar" tags="vblex.pp.m.sg"/>
      <match lemma="sorprender" tags="vblex.pp.m.sg"/>
      <match lemma="implicar" tags="vblex.pp.m.sg"/>
    </or>
    <repeat from="0" upto="1">
      <match tags="adv"/>
    </repeat>
    <match lemma="a" tags="pr"><select lemma="preposicio_buida"/></match>
    <or>
      <match tags="det.*"/>
      <match tags="n.*"/>
      <match tags="adj.*"/>
    </or>
  </rule>

  <!-- romper > trencar / esclafir a  ... -->

  <rule weight="0.5">
    <match lemma="romper" tags="vblex.*"><select lemma="trencar"/></match>
  </rule>

  <rule weight="1.0">
    <match lemma="romper" tags="vblex.*"><select lemma="esclafir"/></match>
    <match lemma="a"/>
    <match tags="*.inf"/>
  </rule>

  <!-- abonar > abonar /adobar -->

  <rule weight="0.5">
    <match lemma="abonar" tags="vblex.*"><select lemma="abonar"/></match>
  </rule>

  <rule weight="1.0">
    <match lemma="abonar" tags="vblex.*"><select lemma="adobar"/></match>
    <repeat from="0" upto="6">
      <or>
        <match tags="*.*"/>
        <match tags="adv"/>
        <match tags="pr"/>
        <match tags="cnjcoo"/>
      </or>
    </repeat> 
    <or>
      <match lemma="tierra" tags="n.*"/>
      <match lemma="terreno" tags="n.*"/>
      <match lemma="campo" tags="n.*"/>
      <match lemma="sembrado" tags="n.*"/>
      <match lemma="planta" tags="n.*"/>
      <match lemma="cultivo" tags="n.*"/>
      <match lemma="árbol" tags="n.*"/>
      <match lemma="hortaliza" tags="n.*"/>
      <match lemma="sembrar" tags="vblex.*"/>
      <match lemma="plantar" tags="vblex.*"/>
      <match lemma="regar" tags="vblex.*"/>
    </or>
  </rule>

  <rule weight="1.0">
    <or>
      <match lemma="tierra" tags="n.*"/>
      <match lemma="terreno" tags="n.*"/>
      <match lemma="campo" tags="n.*"/>
      <match lemma="sembrado" tags="n.*"/>
      <match lemma="planta" tags="n.*"/>
      <match lemma="cultivo" tags="n.*"/>
      <match lemma="árbol" tags="n.*"/>
      <match lemma="hortaliza" tags="n.*"/>
      <match lemma="sembrar" tags="vblex.*"/>
      <match lemma="plantar" tags="vblex.*"/>
      <match lemma="regar" tags="vblex.*"/>
    </or>
    <repeat from="0" upto="6">
      <or>
        <match tags="*.*"/>
        <match tags="adv"/>
        <match tags="pr"/>
        <match tags="cnjcoo"/>
      </or>
    </repeat>
    <match lemma="abonar" tags="vblex.*"><select lemma="adobar"/></match>
  </rule>

  <!-- abono > abonament / adob -->

  <rule weight="0.5">
    <match lemma="abono" tags="n.*"><select lemma="abonament"/></match>
  </rule>

  <rule weight="1.0">
    <match lemma="abono" tags="n.*"><select lemma="adob"/></match>
    <repeat from="0" upto="6">
      <or>
        <match tags="*.*"/>
        <match tags="adv"/>
        <match tags="pr"/>
        <match tags="cnjcoo"/>
      </or>
    </repeat> 
    <or>
      <match lemma="tierra" tags="n.*"/>
      <match lemma="herbicida" tags="n.*"/>
      <match lemma="terreno" tags="n.*"/>
      <match lemma="campo" tags="n.*"/>
      <match lemma="sembrado" tags="n.*"/>
      <match lemma="planta" tags="n.*"/>
      <match lemma="cultivo" tags="n.*"/>
      <match lemma="árbol" tags="n.*"/>
      <match lemma="hortaliza" tags="n.*"/>
      <match lemma="sembrar" tags="vblex.*"/>
      <match lemma="plantar" tags="vblex.*"/>
      <match lemma="regar" tags="vblex.*"/>
      <match lemma="fertilizar" tags="vblex.*"/>
    </or>
  </rule>

  <rule weight="1.0">
    <or>
      <match lemma="tierra" tags="n.*"/>
      <match lemma="herbicida" tags="n.*"/>
      <match lemma="terreno" tags="n.*"/>
      <match lemma="campo" tags="n.*"/>
      <match lemma="sembrado" tags="n.*"/>
      <match lemma="planta" tags="n.*"/>
      <match lemma="cultivo" tags="n.*"/>
      <match lemma="árbol" tags="n.*"/>
      <match lemma="hortaliza" tags="n.*"/>
      <match lemma="sembrar" tags="vblex.*"/>
      <match lemma="plantar" tags="vblex.*"/>
      <match lemma="regar" tags="vblex.*"/>
      <match lemma="fertilizar" tags="vblex.*"/>
    </or>
    <repeat from="0" upto="6">
      <or>
        <match tags="*.*"/>
        <match tags="adv"/>
        <match tags="pr"/>
        <match tags="cnjcoo"/>
      </or>
      <match lemma="abono" tags="n.*"><select lemma="adob"/></match>
    </repeat>
  </rule>



  <!-- reñir > renyir / renyar -->

  <rule weight="0.5">
    <match lemma="reñir" tags="vblex.*"><select lemma="renyir"/></match>
  </rule>

  <rule weight="1.0">
    <match lemma="reñir" tags="vblex.*"><select lemma="renyar"/></match>
    <match lemma="a" tags="pr"/>
  </rule>

  <rule weight="1.0">
    <match tags="prn.pro.*"/>
    <repeat from="0" upto="1">
      <match lemma="haber"/>
    </repeat>
    <match lemma="reñir" tags="vblex.*"><select lemma="renyar"/></match>
  </rule>

  <!-- rayar > ratllar / vorejar -->

  <rule weight="0.5">
    <match lemma="rayar" tags="vblex.*"><select lemma="ratllar"/></match>
  </rule>

  <rule weight="1.0">
    <match lemma="rayar" tags="vblex.*"><select lemma="ranejar"/></match>
    <match lemma="en" tags="pr"/>
    <repeat from="0" upto="3">
      <or>
        <match tags="det.*"/>
        <match tags="adj.*"/>
      </or>
    </repeat>
    <or>
      <match lemma="ismo" tags="n.*"/>
      <match lemma="cencia" tags="n.*"/>
      <match lemma="itud" tags="n.*"/>
      <match lemma="ción" tags="n.*"/>
      <match lemma="idad" tags="n.*"/>
      <match lemma="edad" tags="n.*"/>
      <match lemma="mayoría" tags="n.*"/>
      <match lemma="año" tags="n.*"/>
      <match lemma="amenaza" tags="n.*"/>
      <match lemma="insulto" tags="n.*"/>
      <match lemma="absurdo"/>
      <match tags="num"/>
    </or>
  </rule>

  <rule weight="1.0">
    <match lemma="rayar" tags="vblex.*"><select lemma="vorejar"/></match>
    <repeat from="0" upto="3">
      <or>
        <match tags="det.*"/>
        <match tags="adj.*"/>
      </or>
    </repeat>
    <or>
      <match lemma="ismo" tags="n.*"/>
      <match lemma="cencia" tags="n.*"/>
      <match lemma="itud" tags="n.*"/>
      <match lemma="ción" tags="n.*"/>
      <match lemma="idad" tags="n.*"/>
      <match lemma="edad" tags="n.*"/>
      <match lemma="mayoría" tags="n.*"/>
      <match lemma="año" tags="n.*"/>
      <match lemma="amenaza" tags="n.*"/>
      <match lemma="insulto" tags="n.*"/>
      <match lemma="absurdo"/>
      <match tags="num"/>
    </or>
  </rule>

  <!-- volver > tornar / girar -->

  <rule weight="0.5">
    <match lemma="volver" tags="vblex.*"><select lemma="tornar"/></match>
  </rule>

  <rule weight="2"> <!-- la (re)vista volvió a dedicarle (innecessari si no hi hagués bug en RLX) -->
    <match lemma="volver" tags="vblex.*"><select lemma="tornar"/></match>
    <match lemma="a" tags="pr"/>
    <match tags="*.inf"/>
  </rule>

  <rule weight="1.0">
    <match lemma="volver" tags="vblex.*"><select lemma="girar"/></match>
    <match lemma="en" tags="pr"/>
    <repeat from="0" upto="1">
      <match tags="det.pos.*"/>
    </repeat>
    <match lemma="contra" tags="n.*"/>
  </rule>

  <rule weight="1.0">
    <match lemma="volver" tags="vblex.*"><select lemma="girar"/></match>
    <or>
      <match lemma="en mi contra" tags="adv"/>
      <match lemma="en tu contra" tags="adv"/>
      <match lemma="en su contra" tags="adv"/>
    </or>
  </rule>

  <rule weight="1.0">
    <match lemma="volver" tags="vblex.*"><select lemma="girar"/></match>
    <repeat from="0" upto="2">
      <or>
        <match tags="det.*"/>
        <match tags="adj.*"/>
        <match tags="adv"/>
      </or>
    </repeat>
    <or>
      <match lemma="ojo" tags="n.*"/>
      <match lemma="mirada" tags="n.*"/>
      <match lemma="vista" tags="n.*"/>
      <match lemma="espalda" tags="n.*"/>
    </or>
  </rule>

  <rule weight="1.0">
    <or>
      <match lemma="ojo" tags="n.*"/>
      <match lemma="mirada" tags="n.*"/>
      <match lemma="vista" tags="n.*"/>
    </or>
    <repeat from="0" upto="3">
      <or>
        <match tags="prn.pro.*"/>
        <match tags="adv"/>
        <match lemma="haber"/>
        <match lemma="ir"/>
        <match lemma="a" tags="pr"/>
      </or>
    </repeat>
    <match lemma="volver" tags="vblex.*"><select lemma="girar"/></match>
  </rule>


  <!-- pastel > pastís / pastel -->

  <rule weight="0.5">
    <match lemma="pastel" tags="n.*"><select lemma="pastís"/></match>
  </rule>

  <rule weight="1.0">
    <or>
      <match lemma="tono" tags="n.*"/>
      <match lemma="tonalidad" tags="n.*"/>
      <match lemma="color" tags="n.*"/>
    </or>
    <match lemma="pastel" tags="n.*"><select lemma="pastel"/></match>
  </rule>
    
    <!-- lo que > el que / la qual cosa -->
    <!-- TODO: s'haurà de fer en Constraint Grammar, si es vol millorar -->
  <rule weight="0.5">
    <match lemma="lo que" tags="rel.*"><select lemma="el que"/></match>
  </rule>

  <rule weight="1.0">
    <match tags="cm"/>
    <match lemma="lo que" tags="rel.*"><select lemma="la qual cosa"/></match>
  </rule>

  <rule weight="1.0">
    <match tags="cm"/>
    <match tags="pr" lemma="por"/>
    <match lemma="lo que" tags="rel.*"><select lemma="la qual cosa"/></match>
  </rule>

  <!-- por lo que a ... respecta -->
  <rule weight="0.5">
    <match tags="pr" lemma="respecta"><select lemma="respecta"/></match>
  </rule>

  <rule weight="4.0">
    <match lemma="por" tags="pr"/>
    <match lemma="lo que" tags="rel.*"><select lemma="el que respecta"/></match>
    <match lemma="a" tags="pr"/>
    <repeat from="1" upto="5">
      <or>
        <match tags="*.*"/>
        <match tags="pr"/>
        <match tags="adv"/>
      </or>
    </repeat>
    <match tags="pr" lemma="respecta"><select lemma="preposicio_buida"/></match>
  </rule>

  <rule weight="4.0">
    <match lemma="en" tags="pr"><select lemma="per"/></match>
    <match lemma="lo que" tags="rel.*"><select lemma="el que respecta"/></match>
    <match lemma="a" tags="pr"/>
    <repeat from="1" upto="5">
      <or>
        <match tags="*.*"/>
        <match tags="pr"/>
        <match tags="adv"/>
      </or>
    </repeat>
    <match tags="pr" lemma="respecta"><select lemma="preposicio_buida"/></match>
  </rule> 
    
      <!-- propio  > propi / mateix -->

  <rule weight="0.5">
    <match lemma="propio" tags="adj.*"><select lemma="propi"/></match>
  </rule>

  <rule weight="3">
    <match tags="det.pos.*"/>
    <match lemma="propio" tags="adj.*"><select lemma="propi"/></match>
  </rule>

  <rule weight="1">
    <match tags="det.def.*"/>
    <match lemma="propio" tags="adj.*"><select lemma="mateix"/></match>
    <or>
      <match lemma="autor" tags="n.*"/>
      <match tags="np.*"/>
    </or>
  </rule>

     <!-- por su parte > per part seva, per la seva part -->

  <rule weight="0.5">
    <match lemma="por su parte" tags="adv"><select lemma="per la seua part"/></match>
  </rule>

  <rule weight="0.5">
    <match lemma="por su parte" tags="adv"><select lemma="per la seva part"/></match>
  </rule>

  <rule weight="1">
    <match lemma="por su parte" tags="adv"><select lemma="per part seua"/></match>
    <or>
      <match tags="cm"/>
      <match tags="sent"/>
    </or>
  </rule>

  <rule weight="1">
    <match lemma="por su parte" tags="adv"><select lemma="per part seva"/></match>
    <or>
      <match tags="cm"/>
      <match tags="sent"/>
    </or>
  </rule>

  <!-- palma > palma / palmell -->

  <rule weight="0.5">
    <match lemma="palma" tags="n.*"><select lemma="palma"/></match>
  </rule>

  <rule weight="1.0">
    <match lemma="palma" tags="n.*"><select lemma="palmell"/></match>
    <match lemma="de" tags="pr"/>
    <match tags="det.*"/>
    <match lemma="mano" tags="n.f.*"/>
  </rule>

   <!-- mucho > molt de /molta/molts/moltes -->

  <rule weight="0.5">
    <match lemma="mucho" tags="det.ind.*"><select lemma="molt"/></match>
  </rule>

  <rule weight="1.0">
    <match lemma="mucho" tags="det.ind.m.sg"><select lemma="molt de"/></match>
    <or>
      <match tags="n.m.sg"/>
      <match tags="n.m.sp"/>
      <match tags="n.mf.sp"/>
      <match tags="n.mf.sg"/>
    </or>
  </rule>

  <!-- tipo > tipus / mena -->

  <rule weight="0.5">
    <match lemma="tipo" tags="n.*"><select lemma="tipus"/></match>
  </rule>

  <rule weight="1">
    <match lemma="de" tags="pr"/>
    <or>
      <match lemma="todo"/>
      <match lemma="ninguno" tagger="det.ind.m.sg"/>
    </or>
    <match lemma="tipo" tags="n.m.sg"><select lemma="mena"/></match>
  </rule>

    <rule weight="1">
    <match lemma="ninguno" tagger="det.ind.m.sg"/>
    <match lemma="tipo" tags="n.m.sg"><select lemma="mena"/></match>
    <match lemma="de" tags="pr"/>
  </rule>

  <!-- cebar > encebar / acarnissar -->

  <rule weight="0.5">
    <match lemma="cebar" tags="vblex.*"><select lemma="encebar"/></match>
  </rule>

  <rule weight="1">
    <match tags="prn.pro.*"/>
    <repeat from="0" upto="2">
      <or>
        <match lemma="haber" tags="vbhaver.*"/>
        <match lemma="ir" tags="vblex.*"/>
        <match lemma="a" tags="pr"/>
      </or>
    </repeat>
    <match lemma="cebar" tags="vblex.*"><select lemma="acarnissar"/></match>
    <match lemma="con" tags="pr"/>
  </rule>

  <rule weight="1">
    <match lemma="cebar" tags="vblex.*"><select lemma="acarnissar"/></match>
    <match tags="prn.enc.*"/>
    <match lemma="con" tags="pr"/>
  </rule>
    

